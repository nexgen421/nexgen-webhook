{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "**/*.js": {
            "includeFiles": [
                "node_modules/pg/**/*"
            ]
        }
    }
}
